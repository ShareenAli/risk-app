                <properties>
                  <text value="Change Phase"/>
                </properties>
              </component>
              <component id="ec05b" class="javax.swing.JButton" binding="buttonSaveGame">
                <constraints/>
                <properties>
                  <text value="Save Game"/>
                </properties>
              </component>
            </children>
          </grid>
          <grid id="3af17" binding="panelPlayer" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
            <constraints border-constraint="Center"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="fea46" class="javax.swing.JLabel" binding="labelPlayer">
                <constraints/>
                <properties>
                  <text value="Player"/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
      <scrollpane id="23115" binding="scrollPanelPlayers">
        <constraints border-constraint="South"/>
        <properties>
          <preferredSize width="254" height="100"/>
        </properties>
        <border type="none"/>
        <children>
          <component id="94aea" class="javax.swing.JTable" binding="tablePlayers">
            <constraints/>
            <properties/>
          </component>
        </children>
      </scrollpane>
    </children>
