<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="risk.game.main.phases.PhaseView">
  <grid id="27dc6" binding="panelMain" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="306" height="190"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="99a1f" binding="panelContent" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="914f4" binding="panelPhase" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
            <constraints border-constraint="North"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="970ea" class="javax.swing.JLabel" binding="labelPhase">
                <constraints/>
                <properties>
                  <text value="Phase"/>
                </properties>
              </component>
            </children>
          </grid>
          <grid id="5b94f" binding="panelAction" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
            <constraints border-constraint="South"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="118ff" class="javax.swing.JButton" binding="buttonChange">
